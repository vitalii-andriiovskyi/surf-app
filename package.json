{
  "name": "surfer-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-app:prod": "./node_modules/.bin/ng build --prod",
    "build:server-app:prod": "./node_modules/.bin/ng run surfer-app:server",
    "build-all": "./node_modules/.bin/npm-run-all build:client-app:prod build:server-app:prod build:functions",
    "prerender": "ng run surfer-app:prerender",
    "prerender:debug": "./node_modules/.bin/ts-node --inspect ./prerender.ts",
    "start:express-server": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./server.ts",
    "build:functions": "cd functions && yarn run build",
    "move-index": "mv ./dist/index.html ./functions/lib/index-server.html",
    "firebase:serve": "firebase serve --only hosting,functions",
    "deploy": "firebase deploy",
    "start:local": "npm-run-all build-all move-index firebase:serve",
    "build-and-deploy:prod": "npm-run-all build-all move-index deploy",
    "webpack:server": "webpack --config webpack.server.config.js -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "build:ssr_bak": "yarn run build:client-and-server-bundles && yarn run webpack:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run surfer-app:server",
    "dev:ssr": "ng run surfer-app:serve-ssr",
    "serve:ssr": "node dist/surfer-app-server/main.js",
    "build:ssr": "ng build --prod && ng run surfer-app:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@angular/youtube-player": "^9.2.4",
    "@fortawesome/angular-fontawesome": "0.6.1",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@nguniversal/express-engine": "^9.1.1",
    "bootstrap": "^4.0.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "domino": "^2.1.5",
    "errorhandler": "^1.5.0",
    "express": "~4.16.4",
    "font-awesome": "^4.7.0",
    "http-errors": "~1.6.2",
    "mongodb-client-encryption": "^1.0.1",
    "mongoose": "^5.2.10",
    "morgan": "~1.9.0",
    "nconf": "^0.10.0",
    "ngx-facebook-fb": "^1.0.0",
    "ngx-owl-carousel-o": "3.0.1",
    "require_optional": "^1.0.1",
    "rxjs": "^6.5.5",
    "serve-static": "^1.13.2",
    "ts-loader": "^4.3.0",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/core": "^9.1.7",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^2.8.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "8.10.1",
    "tsconfig-paths": "^3.6.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "typescript": "3.8.3",
    "webpack-cli": "^2.1.3"
  }
}
